local function showNotification(message)
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "XMHUB",
        Text = message,
        Duration = 5,
        Icon = "rbxassetid://80165411282592"
    })
end

showNotification("FUCK YOU")

local function showNotification2(message)
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "XMHUB",
        Text = message,
        Duration = 5,
        Icon = "rbxassetid://130088678394058"
    })
end

showNotification2("欢迎使用XM高级版!")

loadstring(game:HttpGet("https://raw.githubusercontent.com/haohuabnhau-wq/wwfyjfyu/refs/heads/main/lovi"))()
